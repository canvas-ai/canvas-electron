{
    "cache": {
        "enabled": "true"
    },
    "backends": {
        "lmdb": {
            "enabled": true,
            "type": "local",
            "priority": 1,
            "dataTypes": ["note", "document"],
            "driver": "lmdb",
            "driverConfig": {
                "path": "/path/to/lmdb"
            }
        },
        "s3": {
            "enabled": true,
            "type": "remote",
            "priority": 1,
            "dataTypes": ["file"],
            "driver": "s3",
            "driverConfig": {}
        }
    }
}
