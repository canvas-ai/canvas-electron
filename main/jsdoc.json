{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 8,
    "source": {
      "include": [ "." ],
      "exclude": [],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
      "destination": "../docs",
      "recurse": true,
      "readme": "../README.md"
    }
  }
